<?xml version="1.0" encoding="UTF-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TaskSettingShould">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/lock_setting_between_text1"
            android:layout_width="125dp"
            android:layout_height="46dp"
            android:layout_centerInParent="true"
            android:layout_marginBottom="88dp"
            android:background="#D9D9D9"
            android:gravity="center_vertical|center_horizontal"
            android:text="タスクの追加"
            android:textSize="20sp"
            app:layout_constraintBottom_toTopOf="@+id/frame"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/frame"
            android:layout_width="301dp"
            android:layout_height="396dp"
            android:background="@drawable/wakusen"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.456" />

        <TextView
            android:id="@+id/text_view_title"
            android:layout_width="180dp"
            android:layout_height="22dp"
            android:text="・タスク名"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@+id/frame"
            app:layout_constraintEnd_toEndOf="@+id/frame"
            app:layout_constraintHorizontal_bias="0.077"
            app:layout_constraintStart_toStartOf="@+id/frame"
            app:layout_constraintTop_toTopOf="@+id/frame"
            app:layout_constraintVertical_bias="0.022" />

        <EditText
            android:id="@+id/title"
            android:layout_width="270dp"
            android:layout_height="48dp"
            android:ems="10"
            android:hint="ここに入力"
            android:inputType="text"
            app:layout_constraintBottom_toBottomOf="@+id/frame"
            app:layout_constraintEnd_toEndOf="@+id/frame"
            app:layout_constraintHorizontal_bias="0.256"
            app:layout_constraintStart_toStartOf="@+id/frame"
            app:layout_constraintTop_toTopOf="@+id/frame"
            app:layout_constraintVertical_bias="0.067" />

        <TextView
            android:id="@+id/text_view_reason"
            android:layout_width="180dp"
            android:layout_height="22dp"
            android:text="・[タイトル]をする理由"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@+id/frame"
            app:layout_constraintEnd_toEndOf="@+id/frame"
            app:layout_constraintHorizontal_bias="0.074"
            app:layout_constraintStart_toStartOf="@+id/frame"
            app:layout_constraintTop_toTopOf="@+id/frame"
            app:layout_constraintVertical_bias="0.189" />

        <EditText
            android:id="@+id/title_reason"
            android:layout_width="270dp"
            android:layout_height="48dp"
            android:ems="10"
            android:hint="ここに入力"
            android:inputType="text"
            app:layout_constraintBottom_toBottomOf="@+id/frame"
            app:layout_constraintEnd_toEndOf="@+id/frame"
            app:layout_constraintHorizontal_bias="0.258"
            app:layout_constraintStart_toStartOf="@+id/frame"
            app:layout_constraintTop_toTopOf="@+id/frame"
            app:layout_constraintVertical_bias="0.227" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="180dp"
            android:layout_height="22dp"
            android:text="・上記の理由"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@+id/frame"
            app:layout_constraintEnd_toEndOf="@+id/frame"
            app:layout_constraintHorizontal_bias="0.074"
            app:layout_constraintStart_toStartOf="@+id/frame"
            app:layout_constraintTop_toTopOf="@+id/frame"
            app:layout_constraintVertical_bias="0.318" />

        <EditText
            android:id="@+id/reason_of_reason"
            android:layout_width="270dp"
            android:layout_height="48dp"
            android:ems="10"
            android:hint="ここに入力"
            android:inputType="text"
            app:layout_constraintBottom_toBottomOf="@+id/frame"
            app:layout_constraintEnd_toEndOf="@+id/frame"
            app:layout_constraintHorizontal_bias="0.258"
            app:layout_constraintStart_toStartOf="@+id/frame"
            app:layout_constraintTop_toTopOf="@+id/frame"
            app:layout_constraintVertical_bias="0.402" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="180dp"
            android:layout_height="22dp"
            android:text="・メモ"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@+id/frame"
            app:layout_constraintEnd_toEndOf="@+id/frame"
            app:layout_constraintHorizontal_bias="0.074"
            app:layout_constraintStart_toStartOf="@+id/frame"
            app:layout_constraintTop_toTopOf="@+id/frame"
            app:layout_constraintVertical_bias="0.502" />

        <EditText
            android:id="@+id/memo"
            android:layout_width="280dp"
            android:layout_height="43dp"
            android:ems="10"
            android:hint="ここに入力"
            android:inputType="text"
            app:layout_constraintBottom_toBottomOf="@+id/frame"
            app:layout_constraintEnd_toEndOf="@+id/frame"
            app:layout_constraintHorizontal_bias="0.333"
            app:layout_constraintStart_toStartOf="@+id/frame"
            app:layout_constraintTop_toTopOf="@+id/frame"
            app:layout_constraintVertical_bias="0.597" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="180dp"
            android:layout_height="22dp"
            android:text="・期限"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="@+id/frame"
            app:layout_constraintEnd_toEndOf="@+id/frame"
            app:layout_constraintHorizontal_bias="0.074"
            app:layout_constraintStart_toStartOf="@+id/frame"
            app:layout_constraintTop_toTopOf="@+id/frame"
            app:layout_constraintVertical_bias="0.679" />

        <EditText
            android:id="@+id/deadline"
            android:layout_width="280dp"
            android:layout_height="43dp"
            android:ems="10"
            android:hint="ここに入力"
            android:inputType="text"
            app:layout_constraintBottom_toBottomOf="@+id/frame"
            app:layout_constraintEnd_toEndOf="@+id/frame"
            app:layout_constraintHorizontal_bias="0.333"
            app:layout_constraintStart_toStartOf="@+id/frame"
            app:layout_constraintTop_toTopOf="@+id/frame"
            app:layout_constraintVertical_bias="0.781" />

        <Button
            android:id="@+id/button3"
            android:layout_width="103dp"
            android:layout_height="42dp"
            android:text="次へ"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.948"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.976" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>